  <DD_Attribute>
    <name>command_sequence_number</name>
    <version_id>1.0</version_id>
    <local_identifier>ody.command_sequence_number</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>IMG</submitter_name>
    <definition>Numeric identifier for the sequence of commands
      sent to the Odyssey spacecraft which include this THEMIS
      observation.
    </definition>
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Integer</value_data_type>
      <unit_of_measure_type>Units_of_None</unit_of_measure_type>
    </DD_Value_Domain>
  </DD_Attribute>

  <!-- ('IR', 'VIS') -->
  <DD_Attribute>
    <name>detector_id</name>
    <version_id>1.0</version_id>
    <local_identifier>ody.detector_id</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>IMG</submitter_name>
    <definition>Abbreviated name of THEMIS camera used to
      collect this observation.
    </definition>
    <DD_Value_Domain>
      <enumeration_flag>true</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
      <DD_Permissible_Value>
        <value>IR</value>
        <value_meaning>Thermal Emission Imaging System Infrared instrument</value_meaning>
      </DD_Permissible_Value>
      <DD_Permissible_Value>
        <value>VIS</value>
        <value_meaning>Thermal Emission Imaging System Visible instrument</value_meaning>
      </DD_Permissible_Value>
    </DD_Value_Domain>
  </DD_Attribute>

