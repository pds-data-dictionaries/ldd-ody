    </definition>
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Integer</value_data_type>
      <unit_of_measure_type>Units_of_None</unit_of_measure_type>
    </DD_Value_Domain>
  </DD_Attribute>

  <!-- ('IR', 'VIS') -->
  <DD_Attribute>
    <name>detector_id</name>
    <version_id>1.0</version_id>
    <local_identifier>ody.detector_id</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>IMG</submitter_name>
    <definition>Abbreviated name of THEMIS camera used to
